{
  "extends": "../../tsconfig.base.json",  
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "module": "ESNext",
    "jsx": "react-jsx",
    "rootDir": "./src",
    "outDir": "./dist",
    "baseUrl": ".",
    "tsBuildInfoFile": "./dist/.tsbuildinfo.json",

    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true
  },
  "include": [
    "./src",
    "../../.archivelayer/generated"
  ],
  "exclude": [
    "node_modules",
    "./dist/**/*"
  ],
  "references": []
}
  